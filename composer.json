{
    "name": "swiftmade/playback",
    "description": "Implement idempotent endpoints in Laravel à la Stripe",
    "keywords": [
        "laravel",
        "idempotent"
    ],
    "homepage": "https://github.com/swiftmade/playback",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ahmet Özisik",
            "email": "hello@swiftmade.co",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3",
        "illuminate/support": "^8.0",
        "predis/predis": "^1.1"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Swiftmade\\Idempotent\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Swiftmade\\Idempotent\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Swiftmade\\Idempotent\\IdempotentServiceProvider"
            ],
            "aliases": {
                "Idempotent": "Swiftmade\\Idempotent\\IdempotentFacade"
            }
        }
    }
}